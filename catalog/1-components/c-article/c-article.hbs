<article class="c-article js-article {{class}}">
	<div class="row">
		<div class="col-md-1 u-d-none@print">

		</div>

		<header class="c-article_header col-md-10">
			<div class="c-article_openingImage">
				<div class="c-article_openingImageWrapper">
					{{#if c-heroImgOpeningVideo}}
						{{render '@c-heroimg--video16x9' c-heroImgOpeningVideo merge=true}}
					{{else}}
						<picture class="c-article_openingImage_img">
							<source srcset="{{openingImageDesktop}}" media="(min-width: 26em)">
							<img src="{{openingImageSmartphone}}" alt="">
						</picture>
					{{/if}}								
				</div>

				<div class="c-article_openingImage_caption">
					<span>{{openingImageCaption}}</span>
				</div>
			</div>

			{{#if sportResults}}
				<div class="c-article_sportsResults">				
					<div class="c-article_sportsResults_wrapper">
						{{#if c-sportResultsMatch}}{{render '@c-sportresult--match-centred' c-sportResultsMatch merge=true}}{{/if}}
						{{#if c-sportResultsSet}}{{render '@c-sportresult--set-light' c-sportResultsSet merge=true}}{{/if}}
						{{#if c-sportResultsRace}}{{render '@c-sportresult--race-light' c-sportResultsRace merge=true}}{{/if}}

						<div class="c-article_sportsResults_button">
							{{render '@c-label' c-labelTabellino merge=true}}
							{{render '@c-label' c-labelRisultati merge=true}}
						</div>
					</div>
				</div>
			{{/if}}

			<div class="c-article_category">
				{{render '@c-label' c-labelCategory merge=true}}
			</div>

			<hgroup class="c-article_hGroup">
				<h1 class="c-article_title">{{title}}</h1>
				{{#if subTitle}}
					<h2 class="c-article_subTitle">{{subTitle}}</h2>
				{{/if}}
			</hgroup>

			<div class="c-article_service">
				<p class="c-article_date">
					<time datetime="2017-02-14T9:29:00" pubdate>
						<span class="c-article_dateLabel">Ultima modifica:</span>  5 Maggio 2017, ore 14:23
					</time>
				</p>

				<ul class="c-article_service_options">
					<li class="c-article_service_option c-article_service_option-btPrint">
						{{render '@c-btnshare--light' c-btnPrint merge=true}}
					</li>

					<li class="c-article_service_option c-article_service_option-btnShare">
						{{render '@c-btnshare--light' c-btnShare merge=true}}
					</li>				

					<li class="c-article_service_option c-article_service_option-fontSize">
						<span class="c-article_option_smallerFont js-article_option_smallerFont">a</span> 
						<span class="c-article_option_biggerFont js-article_option_biggerFont">A</span>
					</li>
				</ul>		
			</div>
		</header>

		<div class="col-md-1 u-d-none@print">

		</div>

	</div>
	<div class="row" data-sticky_parent>
		<div class="col-md-1 u-d-none@print">

		</div>

		<div class="c-article_colContent col-md-8">
